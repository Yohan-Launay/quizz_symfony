<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

    </head>
    <body>
        {% block body %}
            {% block header %}
                {% include('common/nav.html.twig') %}
            {% endblock %}

            {% block content %}

            {% endblock %}
        {% endblock %}
    </body>
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}

        <script src="https://kit.fontawesome.com/403be62098.js" crossorigin="anonymous"></script>
    {% endblock %}
</html>
